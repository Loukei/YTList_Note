{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/YTVideo",
    "definitions": {
        "YTVideo": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "id": {
                    "type": "string"
                },
                "snippet": {
                    "$ref": "#/definitions/Snippet"
                },
                "contentDetails": {
                    "$ref": "#/definitions/ContentDetails"
                }
            },
            "required": [
                "contentDetails",
                "id",
                "snippet"
            ],
            "title": "YTVideo"
        },
        "ContentDetails": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "duration": {
                    "type": "string",
                    "format": "duration"
                },
                "caption": {
                    "type": "string",
                    "format": "boolean"
                }
            },
            "required": [
                "caption",
                "duration"
            ],
            "title": "ContentDetails"
        },
        "Snippet": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "publishedAt": {
                    "type": "string",
                    "format": "date-time"
                },
                "title": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "channelTitle": {
                    "type": "string"
                },
                "defaultLanguage": {
                    "type": "string"
                },
                "localized": {
                    "$ref": "#/definitions/Localized"
                }
            },
            "required": [
                "channelTitle",
                "defaultLanguage",
                "description",
                "localized",
                "publishedAt",
                "title"
            ],
            "title": "Snippet"
        },
        "Localized": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "title": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                }
            },
            "required": [
                "description",
                "title"
            ],
            "title": "Localized"
        }
    }
}
